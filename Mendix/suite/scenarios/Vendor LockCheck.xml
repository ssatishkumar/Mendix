<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Vendor LockCheck">
 	<test name="HeiPort_Login_LDR">
		<classes>
			<class name="com.mendix.test.LoginScript">
				<methods>
					<include name="tasKill" />
					<include name="openBrowser" />
					<include name="loginAsLDRVendor" />
				</methods>
			</class>
		</classes>
	</test>
	<test name="Vendor_Global_LockCheck">
		<classes>
			<class name="com.mendix.test.VendorScript">
				<methods>
					<include name="Vendor_Global_LockCheck" />
				</methods>
			</class>
		</classes>
	</test>	
</suite> 



